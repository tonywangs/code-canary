  "dependencies": {
    "@dependency-canary/shared": "workspace:*",
    "openai": "^4.28.4",
    "better-sqlite3": "^9.4.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  }
